{
  "name": "hyperledger-fabric-graphql-boilerplate",
  "description": "Hyperledger Fabric GraphQL Boilerplate",
  "version": "0.0.1",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "bcrypt-as-promised": "^1.1.0",
    "cors": "^2.8.4",
    "dataloader": "^1.4.0",
    "date-fns": "^1.29.0",
    "dotenv-safe": "^5.0.1",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "faker": "^4.1.0",
    "graphql": "^0.13.2",
    "graphql-relay": "^0.5.5",
    "hashtable": "git+https://github.com/fabrice102/node-hashtable.git",
    "hfc": "^0.6.5",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^8.2.1",
    "mongoose": "^5.1.1",
    "pretty-format": "^22.4.3",
    "seq-queue": "^0.0.5",
    "sleep": "^5.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.4",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-watch": "^2.0.7",
    "del": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "flow-bin": "^0.72.0",
    "fs-promise": "^2.0.3",
    "jest": "^22.4.4",
    "jest-cli": "^22.4.4",
    "nodemon": "^1.17.4",
    "reify": "^0.15.1",
    "repl": "^0.1.3",
    "repl-promised": "^0.1.0",
    "repl.history": "^0.1.4"
  },
  "engines": {
    "node": "*"
  },
  "keywords": [
    "Blockchain"
  ],
  "license": "MIT",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/entria/hyperledger-fabric-graphql-boilerplate.git"
  },
  "scripts": {
    "build": "rm -rf dist/* && babel src --ignore *.spec.js --out-dir dist --copy-files --source-maps",
    "clear": "babel-node scripts/clear",
    "debug": "DEBUG=hfc node helloblockchain.js",
    "debug:repl": "GRPC_TRACE=all DEBUG=hfc && npm run repl",
    "go:build": "cd src/build-chaincode && GOPATH=$(pwd)/../.. && go build",
    "go:coverage": "cd src/build-chaincode && GOPATH=$(pwd)/../.. && go test -coverprofile cover.out",
    "go:install": "cd src/build-chaincode && GOPATH=$(pwd)/../.. && govend -v",
    "go:test": "cd src/build-chaincode && GOPATH=$(pwd)/../.. && go test",
    "graphql": "GOPATH=$(pwd) && nodemon src/graphql/index.js --exec babel-node",
    "grpc": "GRPC_TRACE=all DEBUG=hfc node helloblockchain.js",
    "hello": "node helloblockchain.js",
    "lint": "eslint --ext js src || exit 0",
    "logs": "docker logs -f $(docker ps -aq | head -n 1)",
    "repl": "GOPATH=$(pwd) && nodemon --config ./repl/nodemon.json ./repl.js --exec babel-node",
    "serve:graphql": "GOPATH=$(pwd) && node ./dist/graphql/index.js",
    "serve:repl": "GOPATH=$(pwd) && babel-node ./repl.js",
    "start": "babel-node ./js/app.js",
    "test": "jest --coverage --runInBand --forceExit",
    "test:watch": "jest --watch --coverage",
    "update-schema": "babel-node ./scripts/updateSchema.js",
    "graphql:build": "docker build -t graphql . || true"
  }
}
